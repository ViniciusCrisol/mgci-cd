{
	"name": "mgci-cd",
	"version": "0.0.1",
	"main": "src/main.ts",
	"engines": {
		"node": "20.18.0"
	},
	"scripts": {
		"build": "npx tsc && tsc-alias",
		"test:unity": "npx jest --projects ./src --coverage",
